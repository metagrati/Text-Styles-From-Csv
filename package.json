    "lint": "eslint --ext .js --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .js --ignore-pattern node_modules --fix .",
    "watch": "npm run build -- --watch"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "*",
    "eslint": "^8.54.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "root": true,
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  }
